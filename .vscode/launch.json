  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Debug CLI Generate",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/cli.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["generate", "--srf-file", "sample_srf.txt"]
    },
    {
      "name": "Python: Debug API Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/api.py",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Debug Test Demo",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test_demo.py",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}